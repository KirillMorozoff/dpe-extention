{
    "manifest_version": 2,
 
    "name": "DPE eXtention",
    "description": "DPE Navigation Bar",
    "version": "1.0",
 
     "browser_action": {
         "default_icon": "icon.png",
         "default_popup": "popup.html"
     },

    "permissions": ["http://*/", "tabs" ], 
    "content_scripts":[
      {
        "matches": ["*://*/*"],
        "js": ["jquery-3.2.1.min.js", "bg.js"]
      }
    ]


}